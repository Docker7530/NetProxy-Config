<service>
    <id>sing-box</id>
    <name>sing-box</name>
    <description>sing-box proxy service</description>
    <executable>E:\MyScoop\Scoop\apps\sing-box\current\sing-box.exe</executable>
    <arguments>run -c "D:\MyService\sing-box\config.json" -D
        "D:\MyService\sing-box"</arguments>
    <onfailure action="restart" delay="5000" />
    <logmode>reset</logmode>
    <logpath>D:\MyService\sing-box\logs</logpath>
</service>
